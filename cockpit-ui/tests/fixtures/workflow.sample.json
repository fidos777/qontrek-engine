{
  "name": "Sample E-Commerce Order Processing Workflow",
  "nodes": [
    {
      "id": "webhook_1",
      "name": "Order Webhook",
      "type": "webhook",
      "position": { "x": 100, "y": 200 },
      "parameters": {
        "httpMethod": "POST",
        "path": "orders/new"
      }
    },
    {
      "id": "set_1",
      "name": "Extract Order Data",
      "type": "set",
      "position": { "x": 300, "y": 200 },
      "parameters": {
        "values": {
          "orderId": "={{ $json.orderId }}",
          "customerId": "={{ $json.customerId }}",
          "total": "={{ $json.total }}"
        }
      }
    },
    {
      "id": "conditional_1",
      "name": "Check Order Value",
      "type": "conditional",
      "position": { "x": 500, "y": 200 },
      "parameters": {
        "conditions": {
          "number": [
            {
              "value1": "={{ $json.total }}",
              "operation": "greaterThan",
              "value2": 1000
            }
          ]
        }
      }
    },
    {
      "id": "http_1",
      "name": "Send to High Value Queue",
      "type": "http_request",
      "position": { "x": 700, "y": 100 },
      "parameters": {
        "url": "https://api.internal/queues/high-value",
        "method": "POST",
        "body": "={{ JSON.stringify($json) }}"
      }
    },
    {
      "id": "http_2",
      "name": "Send to Standard Queue",
      "type": "http_request",
      "position": { "x": 700, "y": 300 },
      "parameters": {
        "url": "https://api.internal/queues/standard",
        "method": "POST",
        "body": "={{ JSON.stringify($json) }}"
      }
    },
    {
      "id": "code_1",
      "name": "Log Processing Complete",
      "type": "code",
      "position": { "x": 900, "y": 200 },
      "parameters": {
        "code": "console.log('Order processed:', $json.orderId);\nreturn $input.all();"
      }
    }
  ],
  "connections": [
    {
      "sourceNodeId": "webhook_1",
      "sourceOutput": 0,
      "targetNodeId": "set_1",
      "targetInput": 0
    },
    {
      "sourceNodeId": "set_1",
      "sourceOutput": 0,
      "targetNodeId": "conditional_1",
      "targetInput": 0
    },
    {
      "sourceNodeId": "conditional_1",
      "sourceOutput": 0,
      "targetNodeId": "http_1",
      "targetInput": 0
    },
    {
      "sourceNodeId": "conditional_1",
      "sourceOutput": 1,
      "targetNodeId": "http_2",
      "targetInput": 0
    },
    {
      "sourceNodeId": "http_1",
      "sourceOutput": 0,
      "targetNodeId": "code_1",
      "targetInput": 0
    },
    {
      "sourceNodeId": "http_2",
      "sourceOutput": 0,
      "targetNodeId": "code_1",
      "targetInput": 0
    }
  ],
  "settings": {
    "executionOrder": "v2",
    "saveManualExecutions": true,
    "callerPolicy": "workflowsFromSameOwner",
    "timezone": "UTC"
  },
  "tags": ["ecommerce", "orders", "processing"]
}
